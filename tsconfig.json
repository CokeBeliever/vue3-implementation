{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "resolveJsonModule": true,
    "outDir": "temp",
    "rootDir": "./",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "paths": {
      "@vue/*": [
        "packages/*/src"
      ],
    }
  },
  "include": [
    "packages/*/src",
    "packages/*/__tests__",
    "packages/*/demo",
  ]
}